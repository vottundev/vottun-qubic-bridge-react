      "react-hook-form": "^7.53.0",
      "react-i18next": "^15.0.1",
      "react-icons": "^5.3.0",
      "sonner": "^1.7.0",
      "tailwind-merge": "^2.5.4",
      "tailwindcss-animate": "^1.0.7",
      "viem": "~2.21.45",
      "wagmi": "^2.12.31",
      "zod": "^3.23.8",
      "zustand": "^4.5.5"
   },
   "devDependencies": {
      "@commitlint/cli": "^19.4.1",
