      "husky": "^9.1.5",
      "jsdom": "^25.0.0",
      "postcss": "^8.4.41",
      "prettier": "^3.3.3",
      "prop-types": "^15.8.1",
      "shx": "^0.3.4",
      "storybook": "^8.2.9",
      "tailwindcss": "^3.4.10",
      "typescript": "^5.5.4",
